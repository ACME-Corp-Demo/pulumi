{
  "version": "0.0.1",
  "name": "plant",
  "resources": {
    "plant:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant::Container"
        },
        "type": {
          "$ref": "#/types/plant:tree/v1:RubberTreeVariety"
        }
      }
    }
  },
  "types": {
    "plant::Container": {
      "type": "object",
      "properties": {
        "size": {
          "$ref": "#/types/plant::ContainerSize"
        },
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant::ContainerColor"},
            {"type": "string"}
          ]
        }
      },
      "required": ["size"]
    },
    "plant::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    }
  },
  "language": {
    "csharp": {},
    "go": {
      "importBasePath": "github.com/pulumi/pulumi-plant/sdk/go/plant",
      "packageImportAliases": {
        "github.com/pulumi/pulumi-kubernetes/sdk/go/plant": "plant"
      }
    },
    "nodejs": {},
    "python": {}
  }
}
